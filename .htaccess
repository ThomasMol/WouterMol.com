RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [L]


RewriteRule ^gallery/([^\.]+) gallery.php?category=$1 [QSA]

RewriteRule ^gallery/([^\.]+)/([^\.]+) gallery.php?category=$1&location=$2 [QSA]


RewriteRule ^gallery/([^\.]+)/([^\.]+)/([^\.]+) gallery.php?category=$1&location=$2&sort=$3 [L]



RewriteCond %{HTTP_HOST} ^www\.woutermol\.com$
RewriteRule ^/?$ "http\:\/\/woutermol\.com\/" [R=301,L]

RewriteCond %{HTTP_USER_AGENT} libwww-perl.* 
RewriteRule .* â€“ [F,L]

RewriteCond %{HTTP_HOST} ^46\.19\.218\.21
RewriteRule (.*) http://woutermol.com/$1 [R=301,L]

<IfModule mod_expires.c>
    ExpiresActive on
 
    ExpiresByType image/jpg "access plus 1 month"
    ExpiresByType image/jpeg "access plus 1 month"
    ExpiresByType image/gif "access plus 1 month"
    ExpiresByType image/png "access plus 1 month"
    ExpiresByType image/svg "access plus 1 month"
</IfModule>

